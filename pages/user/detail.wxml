<view class="container">
  <view class="common-header-xian"></view>
  <!-- 表单 -->
  <form bindreset="formReset" bindsubmit="saveData">
    <view class="address-cont-title b-f b-b">

      <view class="list address-box dis-flex">
        <view class="left-name">
          <text class="f-30">姓名</text>
        </view>
        <view class="right-cont dis-flex">
          <input name="name" placeholder="请输入真实姓名" value="{{detail.name}}"></input>
        </view>
      </view>

      <view class="list address-box dis-flex">
        <view class="left-name">
          <text class="f-30">性别</text>
        </view>
        <view class="right-cont dis-flex">
          <input name="sex" placeholder="请输入真实姓名" value="{{detail.sex}}"></input>
        </view>
      </view>

      <view class="list address-box dis-flex">
        <view class="left-name">
          <text class="f-30">年龄</text>
        </view>
        <view class="right-cont dis-flex">
          <input name="age" placeholder="请输入真实姓名" value="{{detail.age}}"></input>
        </view>
      </view>

      <view class="list address-box dis-flex">
        <view class="left-name">
          <text class="f-30">国籍</text>
        </view>
        <view class="right-cont dis-flex">
          <input name="country" placeholder="请输入真实姓名" value="{{detail.country}}"></input>
        </view>
      </view>

      <view class="list address-box dis-flex">
        <view class="left-name">
          <text class="f-30">手机号</text>
        </view>
        <view class="right-cont dis-flex">
          <input maxlength="11" name="mobile" placeholder="请输入联系电话" type="number" value="{{detail.mobile}}"></input>
        </view>
      </view>

      <view class="list address-box dis-flex ">
        <view class="left-name">
          <text class="f-30">身份证号</text>
        </view>
        <view class="right-cont dis-flex">
          <input name="id_card" placeholder="请输入真实姓名" value="{{detail.id_card}}"></input>
        </view>
      </view>


      <view class="list address-box dis-flex ">
        <view class="left-name">
          <text class="f-30">证件期限</text>
        </view>
        <view class="right-cont dis-flex" style='width: 32%;'>
          <input name="id_card_begintime" placeholder="开始时间" value="{{detail.id_card_begintime}}"></input>
        </view>
        <view class="right-cont dis-flex" style='width: 32%;'>
          <input name="id_card_endtime" placeholder="结束时间" value="{{detail.id_card_endtime}}"></input>
        </view>
      </view>
      
      




    </view>


    <view class="address-cont-title b-f b-b">
      <view class="dis-flex list address-box m-top20 b-b">
        <view class="left-name">
          <text class="f-30">所在地区</text>
        </view>
        <view class="address-cont-box dis-flex">
          <picker mode="region" bindchange="bindRegionChange">
            <text wx:if="{{region}}" class="f-30">{{region}}</text>
            <text wx:else class="f-30 col-7">选择省、市、区</text>
          </picker>
        </view>
      </view>
      <view class="dis-flex list address-box">
        <view class="left-name">
          <text class="f-30">详细地址</text>
        </view>
        <view class="right-cont dis-flex">
          <input name="detail" placeholder="请输入街道小区楼层" type="text" value="{{detail.detail}}"></input>
        </view>
      </view>
    </view>
    <view class="padding-box m-top35 profile-btn">
      <button formType="submit" disabled="{{disabled}}">保存</button>
    </view>
    <!-- <view class="padding-box m-top20 ">
      <button formType="reset">取消</button>
    </view> -->

  </form>

